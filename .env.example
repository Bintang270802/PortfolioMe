# Supabase Configuration
# Get these values from your Supabase project dashboard
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth Configuration (optional)
# Configure in Supabase Auth settings if you want to enable Google login
VITE_GOOGLE_OAUTH_ENABLED=false

# Analytics Configuration (optional)
# Google Analytics Measurement ID (e.g., G-XXXXXXXXXX)
VITE_GA_MEASUREMENT_ID=your_ga_measurement_id_here

# Disable analytics in development (optional)
VITE_DISABLE_ANALYTICS=false